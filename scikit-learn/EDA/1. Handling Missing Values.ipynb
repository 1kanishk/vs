   "source": [
    "from sklearn.impute import SimpleImputer\n",
    "from sklearn.compose import ColumnTransformer\n",
    "from sklearn.pipeline import Pipeline\n",
    "from sklearn.preprocessing import OneHotEncoder, StandardScaler\n",
    "from sklearn.linear_model import LogisticRegression\n",
    "\n",
    "num_cols = ['age','income']\n",
    "cat_cols = ['city','job']\n",
    "\n",
    "\n",
    "\n",
    "preprocessor = ColumnTransformer([\n",
    "    #numerical columns imputing and scaling \n",
    "    ('num', Pipeline([\n",
    "        ('imputer', SimpleImputer(strategy='median')), ('scaler', StandardScaler())\n",
    "    ]), \n",
    "     num_cols),\n",
    "\n",
    "    #catagorical columns imputing and encoding \n",
    "    ('cat', Pipeline([\n",
    "        ('imputer', SimpleImputer(strategy='most_frequent')), ('ohe', OneHotEncoder(handle_unknown='ignore'))\n",
    "    ]), \n",
    "     cat_cols),\n",
    "])\n",
    "\n",
    "\n",
    "\n",
    "clf = Pipeline([\n",
    "    ('pre', preprocessor), ('clf', LogisticRegression())\n",
    "])\n",
    "clf.fit(X_train, y_train)"
   ]
  },
