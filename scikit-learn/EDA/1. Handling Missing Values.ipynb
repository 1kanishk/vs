   "source": [
    "import pandas as pd\n",
    "\n",
    "# detect\n",
    "df.isnull().sum()\n",
    "df['age'].isna().mean()  # proportion missing\n",
    "\n",
    "# drop rows with any NA\n",
    "df_clean = df.dropna(axis=0, how='any')\n",
    "\n",
    "# fill numeric mean\n",
    "df['age'] = df['age'].fillna(df['age'].mean())\n",
    "\n",
    "# fill categorical with 'missing'\n",
    "df['city'] = df['city'].fillna('missing')"
   ]
  },
